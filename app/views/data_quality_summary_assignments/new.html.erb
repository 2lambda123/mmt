<main class="internal" role="main">
  <header>
    <div class="content">
      <div class="eui-breadcrumbs">
        <ol class="eui-breadcrumbs__list">
          <li class="eui-breadcrumbs__item">
            <%= link_to 'Manage CMR', manage_cmr_path %>
          </li>
          <li class="eui-breadcrumbs__item">
            <%= link_to 'Data Quality Summary Assignments', data_quality_summary_assignments_path %>
          </li>
          <li class="eui-breadcrumbs__item">New</li>
        </ol>
      </div>
    </div>
  </header>

  <%= render 'shared/flash_messages' %>

  <div class="row row-content">
    <section>
      <h2>New <%= current_user.provider_id %> Data Quality Summary Assignments</h2>

      <%= form_tag(data_quality_summary_assignments_path, method: :post, class: "data-quality-summary-form", id: "data-quality-summary-assignments-form") do %>
        <%= render partial: "form" %>

        <fieldset>
          <%= link_to 'Cancel', :back, class: 'eui-btn' %>
          <%= submit_tag('Submit', class: 'eui-btn--blue') %>
        </fieldset>
      <% end %>
    </section>
  </div>
</main>
