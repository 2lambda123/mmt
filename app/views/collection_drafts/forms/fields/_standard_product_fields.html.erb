<div class="license-group">
  <% standard_product_true_checked =  !standard_product['True'].nil? %>
  <% standard_product_false_checked =  !standard_product['False'].nil? %>
  <div class="standard-product-type-group standard-product-type-clear-link-radio-fields row sub-fields">
    <!-- Constraint type selector -->
    <%= mmt_label(
      name: 'standard_product_type',
      title: 'Select',
      prefix: name_prefix,
      always_required: false
    ) %>
    <a href="javascript:void(0)" class="clear-radio-button" data-radio-fields="standard-product-type-clear-link-radio-fields" data-clear-fields="standard-product-type-clear-link-clear-fields" data-hide-fields="standard-product-type-clear-link-hide-fields">Clear</a>
    <p class="radio-group"><%= radio_button_tag('standard_product_type_true','True', standard_product_true_checked, class: 'standard_product-type-select') %><label for="standard_product_type_true">True</label></p>
    <p class="radio-group"><%= radio_button_tag('standard_product_type_false','False', standard_product_false_checked, class: 'standard_product-type-select') %><label for="standard_product_type_false">False</label></p>
  </div>

</div>
