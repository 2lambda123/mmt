<div class="preview-collection accordion is-closed">
  <div class="accordion-header">
    <i class="fa fa-angle-down toggle"></i>
    <h4>Acquisition Information Fields:</h4>
  </div>
  <div class="accordion-body">
    <ul class="acquisition-information-preview">

      <%= render partial: 'shared/preview/preview_type', locals: {
          data: metadata['Platforms'],
          name: 'Platforms',
          type: TypesHelper::PlatformsType,
          form_parent: 'data_identification'
      } %>

      <%= render partial: 'shared/preview/preview_type', locals: {
          data: metadata['Projects'],
          name: 'Projects',
          type: TypesHelper::ProjectsType,
          form_parent: 'data_identification'
      } %>

    </ul>
  </div>
</div>
