<div class="row sub-fields">
  <!-- TilingIdentificationSystemName -->
  <%= mmt_text_field(
    name: 'tiling_identification_system_name',
    title: 'Tiling Identification System Name',
    prefix: name_prefix,
    value: tiling_identification_system['TilingIdentificationSystemName'],
    validate: true,
    required: true
   ) %>


  <!-- Coordinate1 -->
  <label class="icon-required">Coordinate 1</label>
  <a href="#help-modal" class="display-modal" tabindex="-1">
    <i class="ed-icon ed-fa-info-circle" data-help-path="definitions/TilingCoordinateType"></i>
    <span class="is-hidden">Help modal for coordinate 1</span>
  </a>
  <%= render partial: 'drafts/forms/type', locals: {
      type: 'tiling_coordinate',
      values: tiling_identification_system['Coordinate1'] || {},
      name_prefix: "#{name_prefix}|coordinate_1|_",
      multiple: false,
      options: {} } %>


  <!-- Coordinate2 -->
  <label class="icon-required">Coordinate 2</label>
  <a href="#help-modal" class="display-modal" tabindex="-1">
    <i class="ed-icon ed-fa-info-circle" data-help-path="definitions/TilingCoordinateType"></i>
    <span class="is-hidden">Help modal for coordinate 2</span>
  </a>
  <%= render partial: 'drafts/forms/type', locals: {
      type: 'tiling_coordinate',
      values: tiling_identification_system['Coordinate2'] || {},
      name_prefix: "#{name_prefix}|coordinate_2|_",
      multiple: false,
      options: {} } %>

</div>
