<label for="<%= remove_pipes(name_prefix) %>person_first_name">First Name</label><i class="fa fa-info-circle"></i>
<%= text_field_tag(name_to_param("#{name_prefix}person_|first_name|"), person['FirstName'], class: 'half-width validate') %>

<label for="<%= remove_pipes(name_prefix) %>person_middle_name">Middle Name</label><i class="fa fa-info-circle"></i>
<%= text_field_tag(name_to_param("#{name_prefix}person_|middle_name|"), person['MiddleName'], class: 'half-width validate') %>

<label for="<%= remove_pipes(name_prefix) %>person_last_name" class="icon-required">Last Name</label><i class="fa fa-info-circle"></i>
<%= text_field_tag(name_to_param("#{name_prefix}person_|last_name|"), person['LastName'], class: 'half-width validate') %>

<label for="<%= remove_pipes(name_prefix) %>person_uuid">Uuid</label><i class="fa fa-info-circle"></i>
<%= text_field_tag(name_to_param("#{name_prefix}person_uuid"), person['Uuid'], class: 'half-width validate') %>
