  <div class="col-6">
    <!-- EndingDateTime -->
    <label for="<%= remove_pipes(name_prefix) %>ending_date_time">Ending Date Time</label><a href="#help-modal" class="display-help-modal" tabindex="-1"><i class="fa fa-info-circle" data-help-path="definitions/RangeDateTimeType/properties/EndingDateTime"></i></a>
    <%= datetime_field_tag(name_to_param("#{name_prefix}|ending_date_time|"), range_date_time['EndingDateTime'], class: 'full-width validate mmt-date-time') %>
  </div>

</div>
