<div class="row sub-fields">

  <div class="col-6">
    <!-- BeginningDateTime -->
    <label for="<%= remove_pipes(name_prefix) %>beginning_date_time" class="icon-required">Beginning Date Time</label><a href="#help-modal" class="display-help-modal" tabindex="-1"><i class="fa fa-info-circle" data-help-path="definitions/RangeDateTimeType/properties/BeginningDateTime"></i></a>
    <%= datetime_field_tag(name_to_param("#{name_prefix}|beginning_date_time|"), range_date_time['BeginningDateTime'], class: 'full-width validate mmt-date-time') %>
