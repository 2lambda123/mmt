<ul class='<%= name_to_class('Responsibility')%>-<%=index%>'>
  <li>
    <% title = defined?(title_override).nil? ? 'Responsibility' : title_override %>
    <%= title %> <%= index + 1 %>

    <!-- Role [r] -->
    <%= render :partial=>'drafts/previews/fields/display_single_select_field_line', :locals=>{:key_name=>'Role', :hash_for_key=>responsibility, options: role_options} %>

    <!-- Party [r] -->
    <%= render :partial=>'drafts/previews/types/party_type', :locals=>{party: responsibility['Party'], title:'Party'} %>
  </li>
</ul>
