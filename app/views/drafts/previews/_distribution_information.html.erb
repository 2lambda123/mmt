<div class="preview-collection accordion is-closed">
  <div class="accordion-header">
    <i class="fa fa-angle-down toggle"></i>
    <h4>Distribution Information Fields:</h4>
  </div>
  <div class="accordion-body">
    <ul class="distribution-information-preview">

      <%= render partial: 'drafts/previews/preview_type', locals: {
          data: draft['RelatedUrl'],
          name: 'RelatedUrl',
          type: TypesHelper::RelatedUrlType
      } %>

      <%= render partial: 'drafts/previews/preview_type', locals: {
          data: draft['Distribution'],
          name: 'Distribution',
          type: TypesHelper::DistributionType
      } %>

    </ul>
  </div>
</div>
