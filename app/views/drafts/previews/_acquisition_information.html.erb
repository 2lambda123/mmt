<div class="preview-collection accordion is-closed">
  <div class="accordion-header">
    <i class="fa fa-angle-down toggle"></i>
    <h4>Acquisition Information Fields:</h4>
  </div>
  <div class="accordion-body">
    <ul class="acquisition-information-preview">

      <%= render partial: 'drafts/previews/preview_type', locals: {
          data: draft['Platform'],
          name: 'Platform',
          type: TypesHelper::PlatformType
      } %>

      <%= render partial: 'drafts/previews/preview_type', locals: {
          data: draft['Project'],
          name: 'Project',
          type: TypesHelper::ProjectType
      } %>

    </ul>
  </div>
</div>
