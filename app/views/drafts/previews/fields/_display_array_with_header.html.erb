<% if !hash_for_key.nil? %>
  <% data_array = hash_for_key[key_name] %>
  <% if !data_array.nil? %>
    <% html_class_name = name_to_class(key_name) %>
    <ul class="<%=html_class_name%>">
      <li>
        <strong><%= (defined?(title_override).nil? ? key_name.titleize : title_override).pluralize(data_array.count) %>:</strong>
        <ul>
          <% data_array.each_with_index do |element_of_array, index|%>
            <li class="<%=html_class_name%>-<%=index%>">
              <%= element_of_array %>
            </li>
          <% end %>
        </ul>
      </li>
    </ul>
  <% end %>
<% end %>