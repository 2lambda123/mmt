<% if !hash_for_key.nil? %>
  <% data_array = hash_for_key[key_name] %>
  <% if !data_array.nil? %>
    <% html_class_name = name_to_class(key_name) %>
    <li class="<%= html_class_name %>">
      <ul class="sub-field">
        <% title = defined?(title_override).nil? ? key_name.titleize : title_override %>
        <% title = defined?(dont_pluralize) ? title : title.pluralize(data_array.count) %>
        <li class="field-name"><%= title %>:</li>
        <% data_array.each_with_index do |element_of_array, index| %>
          <li class="<%= html_class_name %>-<%= index %>">
            <%= element_of_array %>
          </li>
        <% end %>
      </ul>
    </li>
  <% end %>
<% end %>
