<% if !hash_for_key.nil? %>
  <% address_array = hash_for_key[key_name] %>
  <% if !address_array.nil? %>
    <% html_class_name = name_to_class(key_name) %>
    <ul class="<%=html_class_name%>">
      <li>
        <strong><%= (defined?(title_override).nil? ? key_name.titleize : title_override) %>:</strong>
        <ul>
          <% address_array.each_with_index do |street_address_line, index|%>
            <li class="<%=html_class_name%>-<%=index%>">
              <%= street_address_line %>
            </li>
          <% end %>
        </ul>
      </li>
    </ul>
  <% end %>
<% end %>